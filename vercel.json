{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/fonts/ibm-plex-sans-var.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/resume",
      "destination": "/resume.pdf"
    },
    {
      "source": "/projects",
      "destination": "/about"
    },
    {
      "source": "/30",
      "destination": "https://calendly.com/aranganathan/30min"
    }
  ]
}
